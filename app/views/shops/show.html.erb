


<div  class= "col-md-6 col-md-offset-3 text-center">
  <h1><%= @shop.name %></h1>

  <% @shop.barbers.each do |barber| %>
  <div class="col-md-4 text-center" >
   <div><img class="icon icons8-Barber-Chair" src="data:image/png;base64," width="32" height="32"></div>
   <h5><%= link_to barber.name, barber_path(barber) %></h5>
    <% if @shop == current_shop %>
      <%= link_to 'New Barber', new_shop_barber_path(@shop.id) %> |
      <%= link_to 'Edit shop', edit_shop_path(@shop) %> |<br>
    <% end %>
    </div>
  <% end %>
</div>
